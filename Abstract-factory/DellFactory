using System;
using System.Collections.Generic;
using System.Text;

namespace ApstraktnaTvornicaExtraPrimjer
{
    public class DellFactory : ITvFactory
    {
        public ITv CreateTv()
        {
            return new DellTv();
        }

        public IDisplay CreateDisplay()
        {
            return new DellDisplay();
        }
    }
}
