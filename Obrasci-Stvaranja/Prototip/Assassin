using System;
using System.Collections.Generic;
using System.Text;

namespace Obrasci_Stvaranja
{
    public class Assassin : Champion
    {
        string Ability;

        public Assassin(string ability)
        {
            Ability = ability;
        }
        public void Attack()
        {
            Console.WriteLine($"Attack - {ability}");
        }
    }
}
